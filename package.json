{
  "name": "geo-visualisation-components",
  "version": "0.2.0-alpha1",
  "license": "MIT",
  "description": "Geo-visualisation-components is a component library for geospatial visualisations. It is written in TypeScript using Vue 2.",
  "keywords": [
    "geospatial",
    "map",
    "3d",
    "GIS",
    "geojson",
    "web",
    "vue",
    "typescript",
    "ts",
    "component"
  ],
  "repository": "github:GeospatialResearch/geo-visualisation-components",
  "homepage": "https://github.com/GeospatialResearch/geo-visualisation-components",
  "bugs": "https://github.com/GeospatialResearch/geo-visualisation-components/issues",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write lib/",
    "prepublishOnly": "npm run build"
  },
  "type": "module",
  "files": ["dist"],
  "main": "./dist/geo-visualisation-components.umd.cjs",
  "module": "./dist/geo-visualisation-components.js",
  "exports": {
    ".": {
      "import": "./dist/geo-visualisation-components.js",
      "require": "./dist/geo-visualisation-components.umd.cjs"
    }
  },
  "dependencies": {
    "axios": "^1.6.2",
    "bootstrap-vue": "^2.23.1",
    "cesium": "^1.112.0"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.8.0",
    "@tsconfig/node20": "^20.1.4",
    "@types/node": "^20.14.5",
    "@vitejs/plugin-vue": "^5.0.5",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/tsconfig": "^0.5.1",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.23.0",
    "npm-run-all2": "^6.2.0",
    "prettier": "^3.2.5",
    "typescript": "~5.4.0",
    "vite": "^5.3.1",
    "vue": "^3.1.0",
    "vue-tsc": "^2.0.21"
  },
  "peerDependencies": {
    "vue": "^3.1.0"
  },
  "engines": {
    "node": ">=12"
  }
}
